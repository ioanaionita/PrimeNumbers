@model PrimeNumbers.Models.Primes
@{
    ViewBag.Title = "Prime numbers table";
}
<div class="text-center">
    <h1 class="display-4">The prime numbers table:</h1>
</div>
@if (Model != null)
{
    <table class="table">

        <tbody>
            @for (int i = 0; i < Model.Length; i++)
            {
                <tr>
                    @for (int j = 0; j < Model.Length; j++)
                    {
                        <td>
                            @Model.PrimeNumbers[i, j]
                        </td>
                    }
                </tr>
            }

        </tbody>
    </table>
}
